    "res://bin/moonlight-common-c.dll" : ""
}
windows.release = {
    "res://bin/moonlight-common-c.dll" : "",
    "res://bin/moonlight-common-c.dll" : ""
}
linux.debug = {
    "res://bin/moonlight-common-c.so" : "",
    "res://bin/moonlight-common-c.so" : "",
    "res://bin/moonlight-common-c.so" : ""
}
linux.release = {
    "res://bin/moonlight-common-c.so" : "",
    "res://bin/moonlight-common-c.so" : "",
    "res://bin/moonlight-common-c.so" : ""
}